{
  "name": "mucklet-client",
  "version": "1.60.0",
  "description": "A web client for Mucklet.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mucklet/mucklet-client.git"
  },
  "author": "Accipiter Nisus <accipiter@gmail.com>",
  "license": "Apache-2.0",
  "scripts": {
    "start": "npm run hub",
    "client": "cross-env NODE_ENV=development DISABLE_SERVICEWORKER=1 APP_NAME=client webpack-dev-server",
    "client:realm:test": "cross-env NODE_ENV=development DISABLE_SERVICEWORKER=1 APP_NAME=client CFG_NAME=realm_test webpack-dev-server",
    "client:docker": "cross-env NODE_ENV=development DISABLE_SERVICEWORKER=1 APP_NAME=client CFG_NAME=docker webpack-dev-server",
    "client:serviceworker": "cross-env NODE_ENV=development APP_NAME=client CFG_NAME=development_serviceworker webpack-dev-server",
    "hub": "cross-env NODE_ENV=development APP_NAME=hub webpack-dev-server",
    "build": "cross-env NODE_ENV=production webpack",
    "build:hub": "cross-env NODE_ENV=production APP_NAME=hub webpack",
    "build:client": "cross-env NODE_ENV=production APP_NAME=client webpack",
    "build:client:local": "cross-env NODE_ENV=production APP_NAME=client CFG_NAME=local webpack",
    "build:realm:test": "cross-env NODE_ENV=production APP_NAME=client CFG_NAME=prod_realm_test webpack",
    "build:realm:wolfery": "cross-env NODE_ENV=production APP_NAME=client CFG_NAME=prod_realm_wolfery webpack",
    "build:realm:unknown": "cross-env NODE_ENV=production APP_NAME=client CFG_NAME=prod_realm_unknown webpack",
    "test": "jest ./src",
    "lint": "eslint --fix --ext .js src/",
    "test:lint": "eslint --ext .js src/"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.19.4",
    "babel-loader": "^9.0.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.26.0",
    "eslint-plugin-jsdoc": "^46.8.2",
    "eslint-webpack-plugin": "^3.2.0",
    "generate-json-webpack-plugin": "^2.0.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.5.0",
    "mini-css-extract-plugin": "^2.6.1",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "workbox-webpack-plugin": "^7.1.0"
  },
  "dependencies": {
    "@codemirror/commands": "^6.3.0",
    "@codemirror/language": "^6.9.2",
    "@codemirror/state": "^6.3.1",
    "@codemirror/view": "^6.21.4",
    "@lezer/highlight": "^1.1.6",
    "@stripe/stripe-js": "^1.52.0",
    "autocompleter": "^6.1.3",
    "core-js": "^3.26.0",
    "croppie": "^2.6.5",
    "modapp": "^2.7.0",
    "modapp-base-component": "^1.8.1",
    "modapp-l10n": "^1.8.2",
    "modapp-resource": "^1.8.4",
    "modapp-resource-component": "^1.8.3",
    "modapp-utils": "^1.8.0",
    "nouislider": "^15.6.1",
    "resclient": "^2.5.0",
    "simplebar": "^5.3.9"
  }
}
